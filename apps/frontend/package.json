{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "bddgen": "bddgen --config=tests/e2e/playwright.config.ts",
    "test:e2e": "pnpm bddgen && playwright test --config=tests/e2e/playwright.config.ts",
    "test:e2e:headed": "pnpm bddgen && playwright test --config=tests/e2e/playwright.config.ts --headed",
    "test:e2e:debug": "pnpm bddgen && playwright test --config=tests/e2e/playwright.config.ts --debug",
    "test:e2e:ci": "pnpm bddgen && playwright test --config=tests/e2e/playwright.config.ts --project=chromium",
    "test:e2e:all": "PLAYWRIGHT_BDD_TAGS='' pnpm bddgen && playwright test --config=tests/e2e/playwright.config.ts",
    "test:e2e:clean": "cd ../.. && just e2e-restart && cd apps/frontend && pnpm test:e2e:all"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@convex-dev/action-retrier": "^0.2.1",
    "@convex-dev/rate-limiter": "^0.2.1",
    "@convex-dev/react-query": "^0.1.0",
    "@convex-dev/workflow": "^0.3.3",
    "@convex-dev/workpool": "^0.2.18",
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.4",
    "@libar-dev/platform-bc": "workspace:*",
    "@libar-dev/platform-bus": "workspace:*",
    "@libar-dev/platform-core": "workspace:*",
    "@libar-dev/platform-store": "workspace:*",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-router": "^1.153.2",
    "@tanstack/react-router-with-query": "^1.130.17",
    "@tanstack/react-start": "^1.154.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.31.0",
    "lucide-react": "^0.563.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource/geist-mono": "^5.2.7",
    "@fontsource/geist-sans": "^5.2.5",
    "@ladle/react": "^5.1.1",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "playwright-bdd": "^7.0.0",
    "shadcn": "^3.6.2",
    "tailwindcss": "^4",
    "typescript-eslint": "^8.32.0",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.4"
  }
}
